        <rdfs:label>ancestry category</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/HANCESTRO_0290 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/HANCESTRO_0290">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/HANCESTRO_0301"/>
